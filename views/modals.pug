div#winModal(class='modal fade bs-example-modal-sm' tabindex='-1' role='dialog')
  div(class='modal-dialog' role='document')
    div(class='modal-content')
      div(class='modal-header')
        button(type='button' class='close' data-dismiss='modal' aria-label='Close') #[span(aria-hidden='true') &times;] 
        h4(class='modal-title' id='exampleModalLabel') VITÓRIA!
      div(class='modal-body')
        div(class='form-group text-center')
          h2(for='recipient-name' class='control-label') Parabéns. Você ganhou!
      div(class='modal-footer')
        button(type='button' class='btn btn-default' data-dismiss='modal') Fechar
        button#btnNewGame(type='button' class='btn btn-primary') Novo jogo